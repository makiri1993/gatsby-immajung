{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-react",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "never"],
    "no-boolean-literal-compare": false,
    "object-literal-sort-keys": false,
    "import-name": false,
    "jsx-no-multiline-js": false,
    "ordered-imports": false,
    "interface-name": false,
    "variable-name": false
  },
  "rulesDirectory": []
}
